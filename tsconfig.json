{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es6",
      "scripthost"
    ],
    "target": "es5",
    "outDir": "built",
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "built"
  ]
}